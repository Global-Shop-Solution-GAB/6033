Program.Sub.loadDTAll.Start
F.Intrinsic.Control.SetErrorHandler("loadDTAll_Err")
F.Intrinsic.Control.ClearErrors
V.Local.sError.Declare(String)

	v.Local.dMin.Declare(date)
	v.Local.sMin.Declare
	v.Local.dRange.Declare(date)
	v.Local.sRange.Declare
	v.Local.iNegRange.Declare
	v.Local.iIndex.Declare(long)
	V.Local.sSQL.Declare(String,"")

	Gui.FrmQuote.lblStatus.Visible(True)
	Gui.FrmQuote.pbStatus.Visible(True)
	Gui.FrmQuote.pbStatus.Min(0)
	Gui.FrmQuote.pbStatus.Max(100)
	Gui.FrmQuote.pbStatus.Value(15)
	
	f.Intrinsic.String.Concat(v.Local.sSQL,"SELECT DISTINCT A.CUSTOMER,B.NAME,C.NAME_CUSTOMER_SHIP,A.CUSTOMER_PO,A.Quote_NO,A.QUOTE_TYPE,IF(A.DATE_QUOTE='1900-01-01',NULL,A.DATE_QUOTE) AS DATE_QUOTE, ",v.Local.sSQL)
	f.Intrinsic.String.Concat(v.Local.sSQL,"IF(A.DATE_DUE = '1900-01-01',NULL,A.DATE_DUE) AS DATE_DUE, ",V.Local.sSQL)
	f.Intrinsic.String.Concat(v.Local.sSQL,"(SELECT SUM(EXTENSION) FROM V_QUOTE_LINES WHERE A.QUOTE_NO = V_QUOTE_LINES.QUOTE_NO) AS EXTENSION, ",v.Local.sSQL)
	f.Intrinsic.String.Concat(v.Local.sSQL,"A.SALESPERSON,A.USER_5 FROM V_QUOTE_HEADER A LEFT JOIN CUST_PROSP_LIST B ON ",v.Local.sSQL)
	f.Intrinsic.String.Concat(v.Local.sSQL,"A.CUSTOMER = B.PROSPECT LEFT JOIN V_QUOTE_SHIP_TO C ON A.QUOTE_NO=C.QUOTE_NO ORDER BY DATE_DUE DESC;",v.Local.sSQL)

	f.Intrinsic.Control.If(v.Screen.FrmQuote!chkDays.Value,=,1)
		f.Intrinsic.Math.Mult(v.Screen.FrmQuote!txtDays.Text,-1,v.Local.iNegRange)
		f.Intrinsic.Date.DateAdd(D,v.Screen.FrmQuote!txtDays.Text,v.Ambient.Now,v.Local.dRange)
		f.Intrinsic.Date.DateAdd(D,v.Local.iNegRange,v.Ambient.Now,v.Local.dMin)
		f.Intrinsic.String.Format(v.Local.dRange,"YYYY-MM-DD",v.Local.sRange)
		f.Intrinsic.String.Format(v.Local.dMin,"YYYY-MM-DD",v.Local.sMin)
		f.Intrinsic.String.Build("SELECT DISTINCT A.CUSTOMER,B.NAME,C.NAME_CUSTOMER_SHIP,A.CUSTOMER_PO,A.Quote_NO,A.QUOTE_TYPE,IF(A.DATE_QUOTE='1900-01-01',NULL,A.DATE_QUOTE) AS DATE_QUOTE,IF(A.DATE_DUE = '1900-01-01',NULL,A.DATE_DUE) AS DATE_DUE,(SELECT SUM(EXTENSION) FROM V_QUOTE_LINES WHERE A.QUOTE_NO = V_QUOTE_LINES.QUOTE_NO) AS EXTENSION,A.SALESPERSON,A.USER_5 FROM V_QUOTE_HEADER A LEFT JOIN CUST_PROSP_LIST B ON A.CUSTOMER = B.PROSPECT LEFT JOIN V_QUOTE_SHIP_TO C ON A.QUOTE_NO=C.QUOTE_NO WHERE A.DATE_DUE BETWEEN '{0}' AND '{1}' ORDER BY DATE_DUE DESC;",v.Local.sMin,v.Local.sRange,v.Local.sSQL)
	f.Intrinsic.Control.EndIf

	f.Intrinsic.Control.If(v.DataTable.dtAll.Exists,=,True)
		F.data.datatable.close("dtAll")
		F.Data.Dictionary.close("dictType")
	f.Intrinsic.Control.endif

	F.Data.Dictionary.Create("dictType")
	f.Data.Dictionary.AddItem("dictType","A","Approved")
	F.Data.Dictionary.AddItem("dictType","S","Submitted")
	F.Data.Dictionary.addItem("dictType","P","Sent")
	F.data.Dictionary.AddItem("dictType","O","In Process")

	F.Data.DataTable.CreateFromSQL("dtAll","con",v.Local.sSQL,True)
	F.Data.DataTable.FillFromDictionary("dtAll","dictType","Quote_Type")
	
	Gui.FrmQuote.pbStatus.Value(30)
	f.Intrinsic.Control.CallSub(loadGCAll)
	Gui.FrmQuote.pbStatus.Value(45)

F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.Label("loadDTAll_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: PPT_OpenQuoteReview.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	f.Intrinsic.Control.End

